{
  "kind": "collectionType",
  "collectionName": "areas",
  "info": {
    "name": "Areas",
    "description": ""
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "internalId": {
      "type": "string",
      "required": true,
      "maxLength": 15,
      "unique": true
    },
    "area": {
      "type": "string",
      "required": true
    },
    "descripcion": {
      "type": "text",
      "required": true
    },
    "colorPrimario": {
      "type": "string",
      "required": true,
      "maxLength": 20
    },
    "colorSecundario": {
      "type": "string",
      "required": true,
      "maxLength": 20
    },
    "imagen": {
      "model": "file",
      "via": "related",
      "allowedTypes": [
        "images",
        "files",
        "videos"
      ],
      "plugin": "upload",
      "required": false,
      "pluginOptions": {}
    },
    "slogan": {
      "type": "string",
      "required": true,
      "maxLength": 60
    },
    "focus": {
      "type": "enumeration",
      "enum": [
        "Alta",
        "Media",
        "Baja"
      ],
      "default": "Baja",
      "required": true
    },
    "prioridad": {
      "type": "enumeration",
      "enum": [
        "Alta",
        "Media",
        "Baja"
      ],
      "default": "Baja",
      "required": true
    },
    "tipoProyectos": {
      "via": "area",
      "collection": "tipo-proyectos"
    }
  }
}
